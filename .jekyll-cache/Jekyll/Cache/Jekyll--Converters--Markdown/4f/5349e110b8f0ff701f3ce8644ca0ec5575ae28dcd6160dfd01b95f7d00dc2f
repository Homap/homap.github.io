<h1 id="create-orthomcl-sqlite-database">Create OrthoMCL sqlite database</h1>
<p>if [ ! $ORTHO_DB  ] ; then
  ORTHO_DB=‚ÄùorthoMCL.DB.<code class="language-plaintext highlighter-rouge">date +%Y.%m.%d</code>‚Äù; 
fi</p>

<p>echo ‚ÄúdbVendor=sqlite 
